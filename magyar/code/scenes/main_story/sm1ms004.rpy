# game/code/scenes/main_story/sm1ms004.rpy:42
translate magyar sm1ms004_25128656:

    # mc "Oh, that makes sense. What do you do again?"
    mc "Oh, ennek van értelme. Mit csinálsz megint?"

# game/code/scenes/main_story/sm1ms004.rpy:44
