# game/code/scenes/main_story/sm1ms004.rpy:78
translate magyar sm1ms004_89af725f:

    # arj "Sure, I guess."
    arj "Persze, gondolom."

# game/code/scenes/main_story/sm1ms004.rpy:82
translate magyar sm1ms004_3aba90e7:

    # arj "Anyway. Why don't you show me around your shithole so I can get out of here?"
    arj "Na mindegy. Miért nem mutatod meg nekem egy kicsit a szarhelyedet, hogy elmehessek innen?"

# game/code/scenes/main_story/sm1ms004.rpy:84
translate magyar sm1ms004_42f9405a:

