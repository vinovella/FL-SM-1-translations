translate magyar sm1fs_t004_one_hour_later_8c8521a7:

    # mct "What's he talking about!? It looks exactly the same!"
    mct "Miről beszél? Pontosan ugyanúgy néz ki!"

# game/code/scenes/theatre/sm1fs-t004.rpy:312
translate magyar sm1fs_t004_one_hour_later_184163db:

    # mc "I'm glad you think so."
    mc "Örülök, hogy így gondolod."

# game/code/scenes/theatre/sm1fs-t004.rpy:315
translate magyar sm1fs_t004_one_hour_later_1d1ee173:

    # sb "I do."
    sb "Így."

