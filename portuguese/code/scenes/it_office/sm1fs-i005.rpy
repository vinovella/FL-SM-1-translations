    # ag "[mcname]. Nari."
    ag "[mcname]. Nari."

# game/code/scenes/it_office/sm1fs-i005.rpy:855
translate portuguese sm1fs_i005_conference_0f549615:

    # ag "I know this isn't what you signed up for."
    ag "Sei que não é exatamente o que vocês aceitaram fazer."

# game/code/scenes/it_office/sm1fs-i005.rpy:859
translate portuguese sm1fs_i005_conference_5b5f248a:

