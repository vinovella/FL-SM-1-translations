translate portuguese sm1fs_i005_conference_dbad630f:

    # am "Rockets don't fly without fuel. I'll be back from the convenience store in thirty."
    am "Foguetes não voam sem combustível. Vou lá na mercearia e volto em meia hora."

# game/code/scenes/it_office/sm1fs-i005.rpy:837
translate portuguese sm1fs_i005_conference_b3461b7c:

    # ag "Pick me up a Thunderblast when you're there."
    ag "Pega um Thunderblast pra mim enquanto estiver por lá."

# game/code/scenes/it_office/sm1fs-i005.rpy:843
translate portuguese sm1fs_i005_conference_d09eeaf0:

    # am "You got it."
    am "Pode deixar."

# game/code/scenes/it_office/sm1fs-i005.rpy:850
