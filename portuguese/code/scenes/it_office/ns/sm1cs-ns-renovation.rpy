translate portuguese sm1cs_ns_renovation_5e41a918:

    # mc "You'll be living upstairs, Nari."
    mc "Você vai ficar lá em cima, Nari."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:180
translate portuguese sm1cs_ns_renovation_91ddc986:

