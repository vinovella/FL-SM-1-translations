translate portuguese sm1cs_ns_renovation_87a8071d:

    # mc "I'm sure we can find some time to have some fun, Nari."
    mc "Com certeza a gente arruma um tempo pra se divertir, Nari."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:131
translate portuguese sm1cs_ns_renovation_b800fd1e:

    # ns "Good."
    ns "Ótimo."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:137
translate portuguese sm1cs_ns_renovation_8dc471c8:

    # ns "You know how much I like to get sticky with you."
