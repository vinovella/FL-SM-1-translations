    # mct "Wait a minute."
    mct "Espera un minuto."

# game/code/scenes/theatre/sm1fs-t005.rpy:265
translate spanish sm1fs_t005_24341ac4:

    # mc "Wurst Delivery!"
    mc "¡Wurst Delivery!"

# game/code/scenes/theatre/sm1fs-t005.rpy:268
translate spanish sm1fs_t005_3331c2dc:

    # dvh "Huh?"
    dvh "¿Eh?"

# game/code/scenes/theatre/sm1fs-t005.rpy:271
translate spanish sm1fs_t005_6efbba65:

    # km "Worst what?"
    km "¿Worst qué?"

# game/code/scenes/theatre/sm1fs-t005.rpy:274
