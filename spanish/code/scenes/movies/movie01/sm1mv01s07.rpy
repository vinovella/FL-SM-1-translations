    sy "Luego filmaremos las últimas dos escenas."

# game/code/scenes/movies/movie01/sm1mv01s07.rpy:117
translate spanish sm1mv01s07_c6a52e0b:
    # mc "Sounds great."
    mc "Suena genial."

# game/code/scenes/movies/movie01/sm1mv01s07.rpy:118
