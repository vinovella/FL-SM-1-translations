# game/code/scenes/movies/movie01/sm1mv01s02.rpy:977
translate spanish sm1mv01s02_dress_tl_ab9f3ff3:

    # mc "Don't want to waste time we don't have."
    mc "No queremos perder el tiempo que no tenemos."

