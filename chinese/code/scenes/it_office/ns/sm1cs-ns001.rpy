translate chinese sm1cs_ns001_continue_7f280683:

    # mct "He shoots"
    mct "He shoots"

# game/code/scenes/it_office/ns/sm1cs-ns001.rpy:203
translate chinese sm1cs_ns001_continue_7140082a:

