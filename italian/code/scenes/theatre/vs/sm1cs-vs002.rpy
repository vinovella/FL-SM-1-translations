translate italian sm1cs_vs002_rina_worked_hard_d88fe409:

    # vs "Huh?"
    vs "Huh?"

# game/code/scenes/theatre/vs/sm1cs-vs002.rpy:106
translate italian sm1cs_vs002_rina_worked_hard_287d9542:

    # mc "I'm just saying, maybe you might have to work for the theater a little longer than you expected."
    mc "Sto solo dicendo che forse dovrai lavorare al teatro un po' più a lungo di quanto pensassi."

# game/code/scenes/theatre/vs/sm1cs-vs002.rpy:107
translate italian sm1cs_vs002_rina_worked_hard_97c1615b:

    # mc "Before you get your big break."
    mc "Prima di avere la tua grande occasione."

# game/code/scenes/theatre/vs/sm1cs-vs002.rpy:110
translate italian sm1cs_vs002_rina_worked_hard_3351aee2:

    # vs "Darn. Do you think that will affect my plans to run for office later?"
    vs "Accidenti. Pensi che questo influenzerà i miei piani per candidarmi alle elezioni più tardi?"

# game/code/scenes/theatre/vs/sm1cs-vs002.rpy:112
translate italian sm1cs_vs002_rina_worked_hard_fb762248:

