translate italian sm1cs_am002_74a9892a:

    # mc "Buah!"
    mc "Buah!"

# game/code/scenes/it_office/am/sm1cs-am002.rpy:75
translate italian sm1cs_am002_c9b9b334:

    # ag "Sorry for being sneaky, [mcname]."
    ag "Scusa se ti ho spaventato, [mcname]."

# game/code/scenes/it_office/am/sm1cs-am002.rpy:78
