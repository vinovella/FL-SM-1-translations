translate italian sm1cs_ns_renovation_4dd05c50:

    # ns "I hope it is okay that my room will be pretty naked."
    ns "Spero vada bene se la mia stanza sarà piuttosto vuota."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:184
translate italian sm1cs_ns_renovation_4e7ead96:

    # ns "I didn't bring much when I moved here."
    ns "Non ho portato molto quando mi sono trasferita qui."

