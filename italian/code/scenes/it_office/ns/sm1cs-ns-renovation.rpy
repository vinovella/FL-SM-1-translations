translate italian sm1cs_ns_renovation_d2e35562:

    # mct "I might be in trouble when I have both Nari and Stacy living with me."
    mct "Potrei essere nei guai quando avrò sia Nari che Stacy che vivono con me."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:171
translate italian sm1cs_ns_renovation_f5d737cb:

    # mc "Looks like we put a good dent in everything."
    mc "Sembra che abbiamo fatto un buon progresso con tutto."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:174
translate italian sm1cs_ns_renovation_9c0093fb:

    # ns "Where will I be sleeping when I move in?"
    ns "Dove dormirò quando mi trasferirò?"

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:177
translate italian sm1cs_ns_renovation_5e41a918:

    # mc "You'll be living upstairs, Nari."
    mc "Vivrai al piano di sopra, Nari."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:180
translate italian sm1cs_ns_renovation_91ddc986:

    # ns "I'm so excited."
    ns "Sono così eccitata."

