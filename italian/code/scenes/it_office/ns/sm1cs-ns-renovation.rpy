translate italian sm1cs_ns_renovation_e40fc135:

    # ns "But in this case, I should probably have said..."
    ns "Ma in questo caso, avrei dovuto probabilmente dire..."

# game/code/scenes/it_office/ns/sm1cs-ns-renovation.rpy:157
translate italian sm1cs_ns_renovation_b0676de6:

    # ns "I'm so horny."
    ns "Sono così eccitata."

