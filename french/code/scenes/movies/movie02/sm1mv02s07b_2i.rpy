# game/code/scenes/movies/movie02/sm1mv02s07b_2i.rpy:16
translate french sm1mv02s07b_2i_80dcdd87:

    # mc "No, you do not! But she's actually one of your costars in the movie."
    mc "Non, pas encore ! Mais elle est en fait l'une de tes partenaires dans le film."

# game/code/scenes/movies/movie02/sm1mv02s07b_2i.rpy:18
