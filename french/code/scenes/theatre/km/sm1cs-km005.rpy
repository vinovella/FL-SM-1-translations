# game/code/scenes/theatre/km/sm1cs-km005.rpy:33
translate french sm1cs_km005_0a710bfb:

    # mc "Hey Kellie."
    mc "Salut, Kellie."

# game/code/scenes/theatre/km/sm1cs-km005.rpy:34
translate french sm1cs_km005_f8c4c766:

    # mc "Everything alright?"
    mc "Tout va bien ?"

# game/code/scenes/theatre/km/sm1cs-km005.rpy:38
translate french sm1cs_km005_8604af37:

    # km "Totally. Why wouldn't they be?"
    km "Oui, parfaitement. Pourquoi ça n’irait pas ?"

