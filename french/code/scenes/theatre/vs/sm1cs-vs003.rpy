    # kw "Taisia. {w}Taisia!"
    kw "Taisia. {w}Taisia !"

# game/code/scenes/theatre/vs/sm1cs-vs003.rpy:298
translate french sm1cs_vs003_part2_1b400966:

    # tl "What?"
    tl "Quoi ?"

