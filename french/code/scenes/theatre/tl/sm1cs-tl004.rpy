    # tl "Thanks."
    tl "Merci."

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:60
translate french sm1cs_tl004_46d17534:

    # mc "Do... do you have a problem with drinking, Taisia?"
    mc "Tu... tu as un problème avec l’alcool, Taisia ?"

