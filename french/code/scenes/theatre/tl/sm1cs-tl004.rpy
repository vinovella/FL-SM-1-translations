    # tl "I'll give you a kiss."
    tl "Je te donnerai un baiser."

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:291
translate french sm1cs_tl004_2b35b6fe:

    # mc "..."
    mc "..."

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:294
