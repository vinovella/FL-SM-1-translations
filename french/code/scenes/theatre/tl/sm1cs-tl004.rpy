translate french sm1cs_tl004_116179ab:

    # tl "You'll have to go easy on me, it's been a long time since I've played pool."
    tl "Tu devras y aller doucement avec moi, ça fait longtemps que j’ai pas joué au billard."

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:188
translate french sm1cs_tl004_ad909a8e:

    # mc "Hmmm..."
    mc "Hmmm..."

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:191
translate french sm1cs_tl004_1b400966:

    # tl "What?"
    tl "Quoi ?"

# game/code/scenes/theatre/tl/sm1cs-tl004.rpy:197
translate french sm1cs_tl004_525222cd:

    # mc "For some reason, I don't believe you."
    mc "Va savoir pourquoi, mais j’te crois pas."

