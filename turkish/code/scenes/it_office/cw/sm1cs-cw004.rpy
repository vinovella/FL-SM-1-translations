    # mc "*cough*"
    mc "*öksürük*"

# game/code/scenes/it_office/cw/sm1cs-cw004.rpy:287
translate turkish sm1cs_cw004_docks_f46b715f:

    # chw "Want a drink there, [mcname]?"
    chw "Orada bir içki ister misin, [mcname]?"

# game/code/scenes/it_office/cw/sm1cs-cw004.rpy:290
translate turkish sm1cs_cw004_docks_6a05777a:

    # mc "Good god, yes."
    mc "Aman Tanrım, evet."

# game/code/scenes/it_office/cw/sm1cs-cw004.rpy:291
translate turkish sm1cs_cw004_docks_aa50d90e:

    # mc "Ahem. I mean. Yes, I'd love one."
    mc "Öhöm. Yani. Evet, bir tane isterim."

# game/code/scenes/it_office/cw/sm1cs-cw004.rpy:294
