translate turkish sm1cs_am006_arcade_85b7afad:

    # mct "This feels really nice."
    mct "Bu gerçekten hoş bir duygu."

# game/code/scenes/it_office/am/sm1cs-am006.rpy:716
translate turkish sm1cs_am006_arcade_0adbfe46:

    # mct "She's letting me take the lead completely this time."
    mct "Bu sefer tamamen bana inisiyatif veriyor."

