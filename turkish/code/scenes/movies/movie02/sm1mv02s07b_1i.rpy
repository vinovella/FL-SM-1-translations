# game/code/scenes/movies/movie02/sm1mv02s07b_1i.rpy:10
translate turkish sm1mv02s07b_1i_c5a31e9a:

    # mc "That's good to hear!"
    mc "Bunu duymak güzel!"

# game/code/scenes/movies/movie02/sm1mv02s07b_1i.rpy:14
translate turkish sm1mv02s07b_1i_96cb8ed4:

    # ns "It is funny, because I was thinking about you today, too. I was wondering if you would want to do something today together?"
    ns "Komik, çünkü ben de bugün seni düşünüyordum. Bugün birlikte bir şeyler yapmak ister misin diye merak ediyordum?"

# game/code/scenes/movies/movie02/sm1mv02s07b_1i.rpy:16
translate turkish sm1mv02s07b_1i_a963b688:

    # mc "That sounds awesome!"
    mc "Kulağa harika geliyor!"

