    # game/code/scenes/theatre/dvh/sm1cs-dvh003.rpy:89
    old "Flirt with her"
    new "Onunla flört et"

    # game/code/scenes/theatre/dvh/sm1cs-dvh003.rpy:89
    old "Keep your mouth shut"
    new "Ağzını kapat"

