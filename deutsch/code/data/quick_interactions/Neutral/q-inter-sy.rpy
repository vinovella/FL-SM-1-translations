# game/code/data/quick_interactions/Neutral/q-inter-sy.rpy:188
translate deutsch q_inter_sy_20_68beb732:

    # sy "You know, I {i}really{/i} enjoyed filming you and Taisia."
    sy "Weißt du, ich habe es {i}wirklich{/i} genossen, dich und Taisia zu filmen."

# game/code/data/quick_interactions/Neutral/q-inter-sy.rpy:190
translate deutsch q_inter_sy_20_4f43e9fe:

