translate deutsch sm1ms011_01i_it_job_dc16ebcd:

    # mc "Got it."
    mc "Verstanden."

# game/code/scenes/main_story/sm1ms011-01i.rpy:72
translate deutsch sm1ms011_01i_it_job_74a6ce6d:

