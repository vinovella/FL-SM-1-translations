translate deutsch sm1mv01s11i_3eed025a:

    # mc "Hey I just followed your lead."
    mc "Hey, ich bin nur deinem Beispiel gefolgt."

# game/code/scenes/movies/movie01/sm1mv01s11i.rpy:20
translate deutsch sm1mv01s11i_8218548c:

    # sy "Hehe."
    sy "Hehe."

# game/code/scenes/movies/movie01/sm1mv01s11i.rpy:25
translate deutsch sm1mv01s11i_7105311a:

