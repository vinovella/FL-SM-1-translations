    # dc "So formal, [mcname]! Are you in trouble?"
    dc "So förmlich, [mcname]! Hast du Ärger?"

# game/code/scenes/character_scenes/dc/sm1cs-dc-renovation_i.rpy:10
translate deutsch sm1cs_dc_renovation_i_5b79f9dc:

    # mc "Nope. I just like using your title."
