translate deutsch sm1cs_mh005_after_game_eb654fa0:

    # mc "That wasn't really a request..."
    mc "Das war nicht wirklich eine Bitte..."

# game/code/scenes/character_scenes/mh/sm1cs-mh005.rpy:347
translate deutsch sm1cs_mh005_after_game_1547bb22:

    # mh "Oh, so I shouldn't have done it?"
    mh "Oh, also hätte ich das nicht tun sollen?"

# game/code/scenes/character_scenes/mh/sm1cs-mh005.rpy:350
translate deutsch sm1cs_mh005_after_game_2364ab7f:

