    # sy "Thanks, Lyssa."
    sy "Danke, Lyssa."

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:872
translate deutsch sm1cs_mh008_throuple_carousel_c5cb254d:

    # mct "Well, things are definitely off to a good start!"
    mct "Nun, die Dinge haben definitiv einen guten Start hingelegt!"

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:891
translate deutsch sm1cs_mh008_throuple_carousel_f63c75c7:

    # sy "Ooooo, are we going to ride on the ferris wheel!?"
    sy "Ooooo, fahren wir mit dem Riesenrad!?"

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:894
translate deutsch sm1cs_mh008_throuple_carousel_f3d441b0:

    # mc "Well, no visit to the carnival is complete without a ferris wheel ride!"
    mc "Nun, kein Besuch auf dem Jahrmarkt ist komplett ohne eine Fahrt mit dem Riesenrad!"

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:897
translate deutsch sm1cs_mh008_throuple_carousel_45412da6:

    # mh "I couldn't agree more, [mcname]."
    mh "Ich stimme dir voll und ganz zu, [mcname]."

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:913
translate deutsch sm1cs_mh008_throuple_carousel_4467d5d8:

    # mh "I hope you're not afraid of heights, [mcname]!"
    mh "Ich hoffe, du hast keine Höhenangst, [mcname]!"

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:916
translate deutsch sm1cs_mh008_throuple_carousel_3de751db:

