    sy "Das ist so süß! [mcname]! Warum hast du mir keinen gewonnen?"

# game/code/scenes/character_scenes/mh/sm1cs-mh008.rpy:834
translate deutsch sm1cs_mh008_throuple_carousel_2cbf11a3:

    # mc "Well, uhhh..."
    mc "Nun, ähm..."

