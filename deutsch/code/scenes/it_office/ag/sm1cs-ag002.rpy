translate deutsch sm1cs_ag002_0cba0a76:

    # ag "Oh my - were you going to school there while..."
    ag "Oh je – bist du dort zur Schule gegangen, während..."

# game/code/scenes/it_office/ag/sm1cs-ag002.rpy:264
translate deutsch sm1cs_ag002_cc80ecf6:

    # ag "{size=*0.6}While Fetish Locator was a thing?{/size}"
    ag "{size=*0.6}Während es Fetish Locator gab?{/size}"

# game/code/scenes/it_office/ag/sm1cs-ag002.rpy:267
translate deutsch sm1cs_ag002_1f310f6d:

    # mc "Uhm... yep, I was."
    mc "Ähm... ja, das war ich."

