    # ns "Yes. Sister. Stacy."
    ns "Ja. Schwester. Stacy."

# game/code/scenes/it_office/ns/sm1cs-ns010.rpy:142
translate deutsch sm1cs_ns010_7b4db512:

    # ns "I'm Nari. Nari Song."
    ns "Ich bin Nari. Nari Song."

# game/code/scenes/it_office/ns/sm1cs-ns010.rpy:145
translate deutsch sm1cs_ns010_0de915dd:

    # ns "So nice to meet you."
    ns "Schön, dich auch kennenzulernen."

# game/code/scenes/it_office/ns/sm1cs-ns010.rpy:149
translate deutsch sm1cs_ns010_398ae03b:

    # sy "Yeah. Uh... good to be met."
