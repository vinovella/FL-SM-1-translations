translate deutsch sm1cs_ns007_resturant_a995f686:

    # mc "Yes, sounds great. Nari?"
    mc "Ja, klingt gut. Nari?"

# game/code/scenes/it_office/ns/sm1cs-ns007.rpy:188
translate deutsch sm1cs_ns007_resturant_d6e7f61d:

    # ns "Oh. Do you have any... uh."
    ns "Oh. Hast du irgendwelche... äh."

# game/code/scenes/it_office/ns/sm1cs-ns007.rpy:189
translate deutsch sm1cs_ns007_resturant_7c6d591f:

    # ns "April drinks them. What are they called?"
    ns "April trinkt sie. Wie heißen sie?"

# game/code/scenes/it_office/ns/sm1cs-ns007.rpy:190
translate deutsch sm1cs_ns007_resturant_cae96378:

