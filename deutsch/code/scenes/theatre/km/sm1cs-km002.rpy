    new "Alles klar, Kellie"

    # game/code/scenes/theatre/km/sm1cs-km002.rpy:147
    old "Yeah, maybe"
    new "Ja, vielleicht"

    # game/code/scenes/theatre/km/sm1cs-km002.rpy:183
    old "Neutral"
    new "Neutral"

# game/code/scenes/theatre/km/sm1cs-km002.rpy:164
translate deutsch sm1cs_km002_complement_km_5e3be15b:

    # km "[mcname], are you still interested in learning to be a better actor?"
    km "[mcname], bist du immer noch daran interessiert, ein besserer Schauspieler zu werden?"

